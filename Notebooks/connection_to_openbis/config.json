    "objects": {
        "Crystal": {
            "placeholder": "Select crystal...",
            "openbis_object_type": "CRYSTAL",
            "object_type": "raw_material",
            "properties": [
                "$name",
                "material",
                "face",
                "sample_plate",
                "diameter",
                "height",
                "specifications"
            ]
        },
        "Wafer substrate": {
            "placeholder": "Select wafer substrate...",
            "openbis_object_type": "WAFER_SUBSTRATE",
            "object_type": "raw_material",
            "properties": [
                "$name",
                "substrates",
                "sample_plate",
                "diameter",
                "height",
                "thickness"
            ]
        },
        "2D-layer material": {
            "placeholder": "Select 2D-layer material...",
            "openbis_object_type": "2D_LAYERED_MATERIAL",
            "object_type": "raw_material",
            "properties": [
                "$name",
                "sample_plate",
                "layers_2d",
                "substrates",
                "width",
                "length",
                "thickness"
            ]
        },
        "Molecule": {
            "object_type": "material",
            "openbis_object_type": "MOLECULE",
            "properties": [
                "iupac_name",
                "sum_formula",
                "smiles"
            ]
        },
        "Substance": {
            "object_type": "material",
            "openbis_object_type": "SUBSTANCE",
            "properties": [
                "$name",
                "empa_number",
                "batch",
                "amount",
                "has_molecule",
                "comments"
            ]
        },
        "Sputtering": {
            "object_type": "process",
            "openbis_object_type": "SPUTTERING",
            "properties": [
                "duration", 
                "pressure",
                "discharge_voltage",
                "voltage",
                "temperature",
                "angle",
                "current"
            ]
        },
        "Annealing": {
            "object_type": "process",
            "openbis_object_type": "ANNEALING",
            "properties": [
                "duration", 
                "pressure",
                "voltage",
                "temperature",
                "current"
            ]
        },
        "Deposition": {
            "object_type": "process",
            "openbis_object_type": "DEPOSITION",
            "properties": [
                "stabilisation_time", 
                "deposition_time",
                "pressure",
                "substrate_temperature",
                "molecule_temperature",
                "evaporator_slot"
            ]
        },
        "Dosing": {
            "object_type": "process",
            "openbis_object_type": "DOSING",
            "properties": [
                "sum_formula", 
                "duration",
                "pressure",
                "temperature"
            ]
        }
    },
