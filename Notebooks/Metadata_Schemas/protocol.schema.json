{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/protocol.schema.json",
    "title": "Protocol",
    "openBISCode": "PROTOC",
    "description": "Protocol",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "The name of the protocol."
        },
        "description": {
            "title": "Description",
            "type": "string",
            "openBISType": "MULTILINE_VARCHAR",
            "description": "The description of the protocol."
        }
    },
    "additionalProperties": false,
    "required": ["name", "description"]
}