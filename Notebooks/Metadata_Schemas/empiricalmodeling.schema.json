{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/empiricalmodeling.schema.json",
    "title": "Empirical Modeling",
    "openBISCode": "EM",
    "description": "Empirical Modeling schema",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "The name of the Empirical Modeling object."
        },
        "wfmsUUID": {
            "title": "WFMS UUID",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Unique identifier of the simulation object in the WFMS.",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
        },
        "wfmsURL": {
            "title": "WFMS URL",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Link to WFMS.",
            "pattern": "(https?:\/\/(?:www\\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|https?:\/\/(?:www\\.|(?!www))[a-zA-Z0-9]+\\.[^\\s]{2,}|www\\.[a-zA-Z0-9]+\\.[^\\s]{2,})"
        }
    },
    "additionalProperties": false,
    "required": ["name"]
}