{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/tipsensor.schema.json",
    "title": "Tip Sensor",
    "openBISCode": "TIPSENS",
    "description": "Tip Sensor",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "The name of the tip sensor."
        },
        "tipSensorType": {
            "title": "Tip sensor type",
            "type": "string",
            "openBISType": "CONTROLLEDVOCABULARY",
            "enum": ["Tuning fork"],
            "description": "Tip sensor type."
        },
        "wireMaterial": {
            "title": "Wire material",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Wire material."
        },
        "holderId": {
            "title": "Holder ID",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "The label on the sensor."
        },
        "thickness": {
            "title": "Thickness",
            "type": "object",
            "openBISType": "JSON",
            "description": "Thickness of the tip sensor.",
            "properties": {
                "value": {
                    "type": "number"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "length": {
            "title": "Length",
            "type": "object",
            "openBISType": "JSON",
            "description": "Length of the tip sensor.",
            "properties": {
                "value": {
                    "type": "number"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "resonanceFrequency": {
            "title": "Resonance frequency",
            "type": "object",
            "openBISType": "JSON",
            "description": "Resonance frequency of the tip sensor.",
            "properties": {
                "value": {
                    "type": "number"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "qualityFactor": {
            "title": "Quality factor",
            "minimum": 0,
            "type": "integer",
            "openBISType": "INTEGER",
            "description": "Quality factor or Q factor."
        },
        "referenceNumber": {
            "title": "Reference number",
            "type": "integer",
            "openBISType": "INTEGER",
            "description": "Reference number of the tip sensor."
        },
        "deviceParameters": {
            "title": "Device parameters",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Device parameters."
        },
        "receiveDate": {
            "title": "Received date",
            "type": "string",
            "openBISType": "DATE",
            "description": "Date when the tip sensor was received.",
            "format": "date"
        },
        "comments": {
            "title": "Comments",
            "type": "string",
            "openBISType": "MULTILINE_VARCHAR",
            "description": "Comments of the tip sensor."
        }
    },
    "additionalProperties": false,
    "required": ["name", "tipSensorType", "thickness", "length", "resonanceFrequency", "qualityFactor", "receiveDate"]
}