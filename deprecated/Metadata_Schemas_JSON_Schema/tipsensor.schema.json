{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/tipsensor.schema.json",
    "title": "Tip Sensor",
    "openBISCode": "TIPSENS",
    "description": "Tip Sensor",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Name"
        },
        "tip_sensor_type": {
            "title": "Tip sensor type",
            "type": "string",
            "openBISType": "CONTROLLEDVOCABULARY",
            "enum": ["Tuning fork"],
            "description": "Tip sensor type"
        },
        "wire_material": {
            "title": "Wire material",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Wire material"
        },
        "holder_id": {
            "title": "Holder ID",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "The label on the sensor"
        },
        "thickness": {
            "title": "Thickness",
            "type": "object",
            "openBISType": "JSON",
            "description": "Thickness",
            "properties": {
                "value": {
                    "type": "string"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "length": {
            "title": "Length",
            "type": "object",
            "openBISType": "JSON",
            "description": "Length",
            "properties": {
                "value": {
                    "type": "string"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "resonance_frequency": {
            "title": "Resonance frequency",
            "type": "object",
            "openBISType": "JSON",
            "description": "Resonance frequency",
            "properties": {
                "value": {
                    "type": "string"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "quality_factor": {
            "title": "Quality factor",
            "minimum": 0,
            "type": "integer",
            "openBISType": "INTEGER",
            "description": "Quality factor or Q factor."
        },
        "reference_number": {
            "title": "Reference number",
            "type": "integer",
            "openBISType": "INTEGER",
            "description": "Reference number"
        },
        "device_parameters": {
            "title": "Device parameters",
            "type": "string",
            "openBISType": "VARCHAR",
            "description": "Device parameters"
        },
        "receive_date": {
            "title": "Received date",
            "type": "string",
            "openBISType": "DATE",
            "description": "Received date",
            "format": "date"
        },
        "comments": {
            "title": "Comments",
            "type": "string",
            "openBISType": "MULTILINE_VARCHAR",
            "description": "Comments"
        }
    },
    "additionalProperties": false,
    "required": ["name", "tip_sensor_type", "thickness", "length", "resonance_frequency", "quality_factor", "receive_date"]
}
